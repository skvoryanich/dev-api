generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bots {
  id    Int    @id @default(autoincrement())
  name  String @unique
  id_tg String @unique
}

model Users {
  id    Int    @id @default(autoincrement())
  login  String @unique
  password String
  is_admin Boolean
  created_at DateTime   @default(now())
}
